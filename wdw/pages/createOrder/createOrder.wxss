@import '../../template/empty/empty.wxss';
page{
  background-color: #f6f6f6;

}
.container {
  width: 100%;
  padding-bottom: 100rpx;
}
.hide{
  display: none;
}
/* 整个盒子 */
.content {
  width: 100%;
  padding: 24rpx;
  padding-bottom: 0;
}
/* 内容框 */
.table {
  width: 706rpx;
  height: 288rpx;
  background-color: #fff;
  border-radius: 6rpx;
  box-shadow: 0px 0px 8px #ccc;
  margin-bottom: 20rpx;
  padding: 20rpx;
}

.text {
  width: 100%;
  font-size: 24rpx;
  color: #5e5e5e;
}

/* 每个小盒子 */
.box {
  width: 320rpx;
  height: 66rpx;
  background-color: #f6f6f6;
  border-radius: 8rpx;
  box-shadow: 1px 1px 9px #ccc;
  padding: 0 10rpx;
}

/* .div {
  width: 302rpx;
  height: 66rpx;
} */
.div {
width: 100%;
height: 66rpx;
margin-bottom: 30rpx;
}

.box_left {
  float: left;
}

.box_right {
  float: right;
}
.left {
  width: 126rpx;
  height: 66rpx;
  float: left;
  text-align: left;
  line-height: 66rpx;
}
.left.wider{
  width: 156rpx;
}
.left_text {
  display: block;
  width: 186rpx;
  height: 66rpx;
}


.right {
  width: 170rpx; 
  height: 66rpx;
  float: left;
  line-height: 66rpx;
  color: #f75959;
}
.right.thiner{
  width: 140rpx;
}
.thiner .input{
  width: 140rpx;
}
.input {
  display: block;
   width: 170rpx;
  height: 66rpx;
}

.table_bom {
  width: 100%;
  line-height: 50rpx;
  font-size: 24rpx;
  font-weight: bold;
  position: relative;
}

 .table_left {
  margin-left: 26rpx;
} 

.table_color {
  color: #f75959;
  display: inline-block;
}

.table_btn {
  width: 86rpx;
  vertical-align: middle;
  margin: 0 12rpx;
  border-radius: 8rpx;
  text-align: center;
  float: right;
}
/* 复制按钮 */
.copy {
  width: 36rpx;
  height: 36rpx;
  background-color: #576b95;
  position: absolute;
  left: -14rpx;
  top: 6rpx;
}

.copy image {
  width: 36rpx;
  height: 36rpx;
}

/* 删除按钮 */
.del {
  border: 1px solid #f75959;
  color: #f75959;
}

/* 底部样式 */
.footer {
  width: 100%;
  font-size: 24rpx;
  font-weight: bold;
  position: fixed;
  bottom: 0;
  z-index: 999;
}

.footer_top {
  line-height: 56rpx;
  background-color: #9fb9ef;
  text-align: right;
  /* padding: 0 60rpx; */
}

.footer_right {
  color: #fff;
  margin-right: 30rpx;
}

.footer_left {
  color: #fff3d0;
  margin-left: 20rpx;
}

.footer_bom {
  height: 88rpx;
  background-color: gold;
}

.bom_left, .bom_right {
  width: 50%;
  float: left;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  line-height: 88rpx;
  font-weight: normal;
}

.bom_left {
  background-color: #888;
}

.bom_right {
  background-color: #f75959;
}


/* header样式 */

.header {
  /* height: 140rpx; */
  background-color: #9fb9ef;
  margin-bottom: 20rpx;
  border-radius: 8rpx;
  border-top: 1px solid #9fb9ef;
  font-size: 24rpx;
  position: relative;
  padding-top: 24rpx;
  padding-bottom: 24rpx;
  padding-left: 100rpx;
}

.header_top, .header_bom {
  height: 30rpx;
  margin-top: 24rpx;
  line-height: 30rpx;
  color: #fff;
  font-weight: bold;
}
.header_top{
  margin-top: 0;
  height: auto
}
.left_color {
  color: #fff3d0;
  font-size: 30rpx;
}

.left_size {
  font-size: 24rpx;
}

.icon-check {
  width: 36rpx;
  height: 36rpx;
  background-color: #576b95;
  display: inline-block;
  border-radius: 4rpx;
  vertical-align: middle;
}

.icon-check image {
  width: 36rpx;
  height: 36rpx;
}
.h-left{
  /* float: left; */
  position: absolute;
  left: 0;
  text-align: center;
  width: 100rpx;
}
.h-right{
  float: left;
}
.danjia{
  margin-left: 20rpx;
  display: inline-block;
}
.orderName{
  margin:24rpx;
  margin-bottom: 0;
  background-color: #fff;
  height: 86rpx;
  line-height: 86rpx;
  font-size: 26rpx;
  border-radius: 3px;
  padding-left: 24rpx;
}
.orderName .red{
  color: #F75959;
  font-weight: bold;
  margin-right: 40rpx;
}
.allT,.allA{
  margin-right: 40rpx;
  font-weight: bold;
  color: #818181
}