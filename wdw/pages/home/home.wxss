/* .container {} */

/* 头部样式============================================= */
.header {
  width: 100%;
  height: 346rpx;
  background-color: #233142;
}

.header_content {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.header_user {
  width: 100%;
  height: 156rpx;
  margin-top: 52rpx;
}

.headerUser {
  width: 156rpx;
  height: 156rpx;
  margin: 0 auto;
}

.headerUser image {
  width: 100%;
  height: 100%;
}

.header_border {
  width: 100%;
  height: 4rpx;
  margin-top: 30rpx;
}

.headerBorder {
  width: 48rpx;
  height: 4rpx;
  background-color: red;
  margin: 0 auto;
}
.header_text {
  width: 100%;
  height: 28rpx;
  margin-top: 30rpx;
  font-size: 24rpx;
  text-align: center;
  color: #fff;
}

/* 导航栏样式======================================== */

.nav {
  width: 100%;
  height: 90rpx;
  margin-top: 12rpx;
}

.nav_bar {
   width: 100%;
   height: 100%;
}

.nav_bar view {
  width: 33.33%;
  height: 100%;
   display: inline-block; 
  text-align: center;
}

 .nav_icon {
  width: 66rpx;
  height: 66rpx;
  background-color: green;
} 