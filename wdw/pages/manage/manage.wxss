/* table 区域样式====================================================== */

.table {
  border-bottom: 9rpx solid #ececec;
}

.table_left, .table_right {
  width: 50%;
}

.table_left {
  height: 100%;
  padding: 20rpx;
  float: left;
  font-size: 28rpx;
}

.size_color {
  color: #4f4f4f;
}

.table_right {
  height: 100%;
  float: left;
  padding: 20rpx;
  font-size: 28rpx;
}

.choice {
  width: 330rpx;
  height: 66rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  padding: 0 0 0 20rpx;
  border: 1rpx solid #7e7e7e;
  border-radius: 10rpx;
}

.input {
  display: inline-block;
  height: 100%;
  width: 100%;
  font-size: 22rpx;
}

.icon_position {
  position: relative;
}

.icon {
  width: 0;
  height: 0;
  border-top: 14rpx solid #5f5c5b;
  border-bottom: 14rpx solid #fff;
  border-left: 14rpx solid #fff;
  border-right: 14rpx solid #fff;
  position: absolute;
  top: 26rpx;
  right: 20rpx;
}

.btn {
  width: 330rpx;
  height: 66rpx;
  margin-top: 80rpx;
}

.btn_search, .btn_eliminate {
  width: 46%;
  text-align: center;
  line-height: 66rpx;
  border-radius: 10rpx;
}

.btn_search {
  float: left;
  background-color: #f75959;
  color: #fff;
}

.btn_eliminate {
  float: right;
  border: 1rpx solid #576b95;
  color: #576b95;
}

/*======================复制部分==========================================================  */

/* header 区域样式 ===========================================*/

.header {
  width: 708rpx;
  height: 198rpx;
  background-color: #9fb9ef;
  margin: 0 auto;
  margin-top: 30rpx;
  border-radius: 10rpx;
  padding: 30rpx;
  margin-bottom: 30rpx;
}

.odd, .state, .site {
  height: 24rpx;
  font-size: 24rpx;
  font-weight: bold;
  line-height: 24rpx;
}

.state {
  margin-top: 30rpx;
}

.site {
  width: 250rpx;
  text-align: center;
  font-size: 24rpx;
  color: #fff;
  font-weight: highter;
  margin: 34rpx auto 0;
}

.odd_left {
  color: #fff3d0;
}

.odd_right {
  color: #fff;
  padding-right: 70rpx;
  padding-left: 10rpx;
}

/* nav 区域样式============================================================= */

.nav {
  width: 100%;
  height: 90rpx;
  border-bottom: 9rpx solid #ececec;
  overflow-x: auto;
  white-space: nowrap;
  padding: 0 20rpx;
  background-color: pink;
}

.nav_stay, .nav_already, .nav_abnormal {
  margin-right: 60rpx;
  display: inline-block;
  line-height: 76rpx;
  font-size: 26rpx;
  color: #4f4f4f;
}

.nav_stay {
  color: #f75959;
  border-bottom: 6rpx solid red;
}


  /*提示信息  ===================================*/

  .hint {
    line-height: 26rpx;
    margin-top: 50rpx;
    color: #939292;
    font-size: 26rpx;
    text-align: center;
  }