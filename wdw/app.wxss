/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column; 
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.clearfix:after{
  display: block;
  content: '';
  clear: both;
  zoom: 1;
}
page,view,text{
  box-sizing: border-box;
}

page {
  padding: 0;
  margin: 0;
}